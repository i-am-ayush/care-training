<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
    <listener-class>listener.OnStart</listener-class>
  </listener>

  <servlet>
    <servlet-name>RegisterSeeker</servlet-name>
    <servlet-class>registerseeker</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterSitter</servlet-name>
    <servlet-class>registersitter</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>logout</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProfileRedirect</servlet-name>
    <servlet-class>ProfileRedirect</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditProfileForm</servlet-name>
    <servlet-class>EditProfileForm</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditSeekerProfile</servlet-name>
    <servlet-class>EditSeekerProfile</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditSitterProfile</servlet-name>
    <servlet-class>EditSitterProfile</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteSitterApplication</servlet-name>
    <servlet-class>deletesitterapplication</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CloseAccount</servlet-name>
    <servlet-class>closeaccount</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServeAccount</servlet-name>
    <servlet-class>accountserve</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CloseJob</servlet-name>
    <servlet-class>CloseJob</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PostNewJob</servlet-name>
    <servlet-class>PostNewJob</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ApplicantList</servlet-name>
    <servlet-class>ApplicantList</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditJob</servlet-name>
    <servlet-class>EditJobDetails</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ApplyNewJob</servlet-name>
    <servlet-class>ApplyNewJob</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ApplyJob</servlet-name>
    <servlet-class>ApplyJob</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Search</servlet-name>
    <servlet-class>Search</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SitterAccount</servlet-name>
    <servlet-class>SitterAccount</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SeekerAccountServlet</servlet-name>
    <servlet-class>SeekerAccountServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditJobDetailsFormServlet</servlet-name>
    <servlet-class>EditJobDetailsFormServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SitterAccount</servlet-name>
    <url-pattern>/sitteraccount</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditJobDetailsFormServlet</servlet-name>
    <url-pattern>/editjobdetailsform</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SeekerAccountServlet</servlet-name>
    <url-pattern>/seekeraccount</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterSeeker</servlet-name>
    <url-pattern>/registerseeker</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterSitter</servlet-name>
    <url-pattern>/registersitter</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProfileRedirect</servlet-name>
    <url-pattern>/profileredirect</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditProfileForm</servlet-name>
    <url-pattern>/editprofileform</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditSeekerProfile</servlet-name>
    <url-pattern>/editseekerprofile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditSitterProfile</servlet-name>
    <url-pattern>/editsitterprofile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteSitterApplication</servlet-name>
    <url-pattern>/deletesitterapplication</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CloseAccount</servlet-name>
    <url-pattern>/closeaccount</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServeAccount</servlet-name>
    <url-pattern>/accountserve</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CloseJob</servlet-name>
    <url-pattern>/closejob</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PostNewJob</servlet-name>
    <url-pattern>/postnewjob</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ApplicantList</servlet-name>
    <url-pattern>/applicantlist</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditJob</servlet-name>
    <url-pattern>/editjobdetails</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ApplyNewJob</servlet-name>
    <url-pattern>/applynewjob</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ApplyJob</servlet-name>
    <url-pattern>/applyjob</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Search</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <el-enabled>true</el-enabled>
      <scripting-enabled>true</scripting-enabled>
    </jsp-property-group>
  </jsp-config>
</web-app>
